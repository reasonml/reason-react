{
  "name": "reason-react",
  "version": "0.8.0-dev.3",
  "description": "React bindings for Reason",
  "main": "lib/js/src/ReasonReact.js",
  "scripts": {
    "build": "bsb -make-world",
    "start": "bsb -make-world -w",
    "clean": "bsb -clean-world",
    "test": "yarn clean && yarn build",
