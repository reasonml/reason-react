  "keywords": [
    "reasonml",
    "bucklescript",
    "react"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reasonml/reason-react.git"
  },
  "homepage": "https://reasonml.github.io/reason-react/",
  "devDependencies": {
    "bs-platform": "7.1.1"
  },
  "peerDependencies": {
    "bs-platform": "^7.1.1",
    "react": "^16.8.1",
    "react-dom": "^16.8.1"
  }
}
